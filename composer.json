{
    "name": "forestry/:package_name",
    "description": ":package_description",
    "keywords": [
        "forestry",
        ":package_name"
    ],
    "homepage": "https://github.com/ForestryCodes/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "homepage": ":author_website",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
